{
    "name": "Trustnav",
    "version": "0.1.2",
    "description": "Trustnav",
    "browser_action": {
        "default_popup": "./popup/popup.html",
        "default_icon": "./popup/img/logo.png"
    },
    "content_scripts": [
    {
    "all_frames": true,
    "js": [
        "adblock/ext/common.js",
        "adblock/ext/content.js",
        "adblock/cssProperties.js",
        "adblock/include.preload.js"
    ],
    "match_about_blank": true,
    "matches": [
        "http://*/*",
        "https://*/*"
    ],
    "run_at": "document_start"
    },{
        "all_frames": true,
        "js": [
            "./components/jquery/dist/jquery.min.js",
            "./content_scripts/tnComponent.js"
        ],
        "match_about_blank": true,
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "include_globs": [ "*trustnav*" ],
        "run_at": "document_end"
    },
    {
        "all_frames": true,
        "js": [
            "adblock/include.postload.js"
        ],
        "match_about_blank": true,
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "run_at": "document_end"
    }],
    "icons": {
        "128": "./popup/img/logoGrande.png"
    },
    "background": {
        "page": "./background/background.html"
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs",
        "proxy",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "\u003Call_urls>",
        "storage",
        "cookies",
        "http://localhost/",
        "contextMenus",
        "unlimitedStorage",
        "notifications"
    ],
    "manifest_version": 2,
    "storage": {
        "managed_schema": "adblock/managed-storage-schema.json"
    }
}
